{
  "schemaVersion": 1,
  "id": "embeddium",
  "version": "${version}",
  "name": "Embeddium",
  "description": "Embeddium is a fork of Sodium which focuses on mod compatibility",
  "authors": [
    {
      "name": "embeddedt"
    },
    {
      "name": "jellysquid3",
      "contact": {
        "email": "jellysquid@pm.me",
        "homepage": "https://jellysquid.me"
      }
    }
  ],
  "contributors": [
    "@IMS212",
