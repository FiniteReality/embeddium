    "@TwistedZero",
    "@Leo40Git"
  ],
  "contact": {
    "homepage": "https://github.com/embeddedt/embeddium",
    "sources": "https://github.com/embeddedt/embeddium",
    "issues": "https://github.com/embeddedt/embeddium/issues"
  },
  "license": "LGPL-3.0-only",
  "icon": "icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.jellysquid.mods.sodium.client.SodiumClientMod"
    ],
    "preLaunch": [
      "me.jellysquid.mods.sodium.client.SodiumPreLaunch"
    ]
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/rN9Y7caguP",
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/embeddium",
        "modmenu.modrinth": "https://modrinth.com/mod/embeddium"
      }
    }
  },
  "accessWidener": "embeddium.accesswidener",
  "mixins": [
    "embeddium.mixins.json"
  ],
  "depends": {
    "minecraft": "1.16.1",
    "fabricloader": ">=0.15.6",
    "fabric-rendering-fluids-v1": "*"
  },
  "breaks": {
    "optifabric": "*",
    "canvas": "*"
  }
}
