    "loom:injected_interfaces": {
      "net/minecraft/class_238": ["org/embeddedt/embeddium/fabric/injectors/AABBInjector"],
      "net/minecraft/class_1087": ["net/neoforged/neoforge/client/extensions/IBakedModelExtension"],
      "net/minecraft/class_777": ["org/embeddedt/embeddium/fabric/injectors/BakedQuadInjector"],
      "net/minecraft/class_1920": ["org/embeddedt/embeddium/fabric/injectors/BlockAndTintGetterInjector"],
      "net/minecraft/class_2586": ["org/embeddedt/embeddium/fabric/injectors/BlockEntityInjector"],
      "net/minecraft/class_827": ["org/embeddedt/embeddium/fabric/injectors/BlockEntityRendererInjector"],
      "net/minecraft/class_4970\u0024class_4971": ["org/embeddedt/embeddium/fabric/injectors/BlockStateBaseInjector"],
      "net/minecraft/class_3611": ["org/embeddedt/embeddium/fabric/injectors/FluidInjector"],
      "net/minecraft/class_2818": ["org/embeddedt/embeddium/fabric/injectors/LevelChunkInjector"],
      "net/minecraft/class_1937": ["org/embeddedt/embeddium/fabric/injectors/LevelInjector"],
      "net/minecraft/class_1921": ["org/embeddedt/embeddium/fabric/injectors/RenderTypeInjector"],
      "net/minecraft/class_4588": ["org/embeddedt/embeddium/fabric/injectors/VertexConsumerInjector"]
    }
  },
  "mixins": [
    "embeddium.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.6"
  },
  "breaks": {
    "sodium": "*",
    "optifabric": "*",
    "canvas": "*"
  }
}
